<Page x:Class="Sims_Hospital_Zdravo.View.ViewDoctor.LabaratoryResultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sims_Hospital_Zdravo.View.ViewDoctor"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LabaratoryResultsPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="1" Name="LabaratoryDataGrid" ItemsSource="{Binding LabaratoryTests}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Date" Binding="{Binding Date}">
                    
                </DataGridTextColumn>
                <DataGridTextColumn Header="Test"/>
                <DataGridTextColumn Header="Result"/>
            </DataGrid.Columns> 

        </DataGrid>
    </Grid>
</Page>
